{
  "name": "studyhub",
  "version": "1.0.0",
  "description": "StudyHub - 함께 공부하는 즐거움",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "d1:create": "wrangler d1 create studyhub-db",
    "d1:migrate:local": "wrangler d1 migrations apply studyhub-db --local",
    "d1:migrate:remote": "wrangler d1 migrations apply studyhub-db --remote",
    "d1:query:local": "wrangler d1 execute studyhub-db --local --command",
    "d1:query:remote": "wrangler d1 execute studyhub-db --remote --command",
    "secret:jwt": "wrangler secret put JWT_SECRET"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "d1",
    "study",
    "education"
  ],
  "author": "StudyHub Team",
  "license": "MIT",
  "devDependencies": {
    "wrangler": "^3.0.0"
  }
}
